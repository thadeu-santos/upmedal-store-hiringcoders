  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },

  "search-result-layout.customQuery#corinthians": {
    "blocks": ["search-result-layout.desktop#shelf-home"],
    "props": {
      "querySchema": {
        "mapField": "fq",
        "queryField": "kit-upmedal/corinthians",
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "skip"
      }
    }
  },

